<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/navigation_resource_file"
    app:startDestination="@id/chatRoomsFragment">

    <fragment
        android:id="@+id/chatRoomsFragment"
        android:name="com.example.circle.ui.chatRooms.ChatRoomsFragment"
        android:label="ChatRoomsFragment" >
        <action
            android:id="@+id/action_chatRoomsFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
        <action
            android:id="@+id/action_chatRoomsFragment_to_AI_CHAT_PLACEHOLDER"
            app:destination="@id/AI_CHAT_PLACEHOLDER" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.example.circle.ui.chatRooms.chats.ChatFragment"
        android:label="ChatFragment" />
    <fragment android:id="@+id/AI_CHAT_PLACEHOLDER" />
</navigation>